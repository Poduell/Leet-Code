
class Solution {
    public String reverseWords(String s) {
        StringBuilder str=new StringBuilder();
        String[] now=s.split(" ");
        for(int i=now.length-1;i>=0;i--){
            if(now[i]!=""){
                str.append(now[i]);
                str.append(" ");
            }
        }
        return str.toString().trim();
    }
}
